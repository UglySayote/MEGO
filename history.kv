MDScreen:
    name: "history"
    MDFloatLayout:
        md_bg_color: kivy.utils.get_color_from_hex("#FDA300")

        MDFloatLayout:
            md_bg_color: kivy.utils.get_color_from_hex("#1F4762")
            size_hint: 1, .07
            pos_hint: {"center_x": .5, "center_y": .97}
        MDIconButton:
            icon: "icons/white/chevron-left.png"
            user_font_size: ".1dp"
            pos_hint: {"center_x": .05, "center_y": .97}
            on_release:
                app.decide_home()
                root.manager.transition.direction = "right"
        MDLabel:
            text: "Transaction History"
            pos_hint: {"center_x": .6, "center_y": .97}
            halign: "left"
            font_size: "20dp"
            font_name: "fonts/Nunito-Bold.ttf"
            color: kivy.utils.get_color_from_hex("#FEDFA6")
        Button:
            background_color: 0,0,0,0
            color: kivy.utils.get_color_from_hex("#FDA300")
            size_hint: .9, .11
            pos_hint: {"center_x": .5, "center_y": .80}
            canvas.before:
                Color:
                    rgba: kivy.utils.get_color_from_hex("#FEDFA6") if self.state == "normal" else kivy.utils.get_color_from_hex("#baa377")
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [10]
        MDLabel:
